/*#include <stdio.h>*/

/*int main (void){*/

/*int x;*/
/*int y;*/
/*int temp;*/

/*printf("Enter value for variable X:\n");*/
/*scanf("%d", &x);*/

/*printf("Enter value for variable Y:\n");*/
/*scanf("%d", &y);*/

/*printf("Before: X = %d and Y = %d\n", x, y);*/

/*temp = x;*/
/*x = y;*/
/*y = temp;*/

/*printf("After: X = %d and Y = %d\n", x, y);*/
/*}*/

