/*#include <stdio.h>*/

/*int main (void) {*/

/*int num;*/
/*long double fact = 1;*/

/*printf("Enter a number here\n");*/
/*scanf("%d", &num);*/
/*while(num < 0){*/
/*    printf("Your number cannot be negative. Please enter a new number.\n");*/
/*    scanf("%d", &num);*/
/*}*/
/*for(int i = 1; i <= num; i++){*/
/*    fact *= i;*/
/*    if(i == num){*/
/*        printf("The factorial of %d is %.2Lf\n", num, fact);*/
/*    };*/
/*  };*/
/*};*/

