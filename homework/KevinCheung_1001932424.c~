/*#include <stdio.h>*/

/*int main (void){*/
/*    int sn = 0;*/
/*    int a1 = 4;*/
/*    int d = 5;*/
/*    int n = 50;*/
/*    int count = 1;*/
/*    while(count < n){*/
/*        printf("The %d term is %d.\n", count, a1 + (count - 1) * d);*/
/*        count++;*/
/*    };*/
/*    if(count == n){*/
/*        int an = a1 + (count - 1) * d;*/
/*        printf("The %d term is %d.\n", count, an);*/
/*        sn = (count * (a1 + an))/2;*/
/*        printf("The sum of the first %d terms is %d.\n", n, sn);*/
/*    };*/
/*};*/

